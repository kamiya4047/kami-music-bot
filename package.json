{
  "name": "kami-music-bot",
  "version": "1.0.0-beta.2",
  "description": "Kamiya's music bot",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "rollup -c",
    "start": "node .",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "toc": "i18next-resources-for-ts toc -i ./src/localization/zh-TW -o ./src/@types/resources.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull"
  },
  "author": {
    "name": "Kamiya",
    "email": "kamiya@kamiya.app",
    "url": "https://kamiya.app"
  },
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.7",
    "arktype": "^2.1.22",
    "bufferutil": "^4.0.9",
    "chalk": "5.6.2",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "ffmpeg-static": "^5.2.0",
    "iso8601-duration": "^2.1.3",
    "lowdb": "^7.0.1",
    "nanoid": "^5.1.5",
    "pino": "^9.10.0",
    "pino-pretty": "^13.1.1",
    "postgres": "^3.4.7",
    "prism-media": "^1.3.5",
    "scdl-core": "^1.1.0",
    "sodium-native": "^5.0.9",
    "youtube-dl-exec": "^3.0.25"
  },
  "devDependencies": {
    "@kamiya4047/eslint-plugin-pretty-import": "^0.1.6",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@stylistic/eslint-plugin": "^5.4.0",
    "builtin-modules": "^5.0.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.36.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "jiti": "^2.6.0",
    "node-gyp": "^11.4.2",
    "rollup": "^4.52.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  },
  "trustedDependencies": [
    "@discordjs/opus",
    "sodium-native",
    "bufferutil",
    "es5-ext",
    "esbuild",
    "ffmpeg-static",
    "utf-8-validate",
    "youtube-dl-exec"
  ]
